<Project InitialTargets="BeforeBuild" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PropsFile />
    <ProxyGeneratorVersion>$version$</ProxyGeneratorVersion>
  </PropertyGroup>
  <Import Project="$(projectDir)$(PropsFile)" Condition="'$(PropsFile)' != ''" />
  <Target Name="BeforeBuild" >
    <PropertyGroup>
      <ProxyGeneratorExecutable>$(solutionDir)packages\Birch.Swagger.ProxyGenerator.$(ProxyGeneratorVersion)\tools\ProxyGenerator\Birch.Swagger.ProxyGenerator.exe</ProxyGeneratorExecutable>
    </PropertyGroup>
    <Exec Command='"$(ProxyGeneratorExecutable)" -BaseDirectory "$(projectDir)\"' />
  </Target>
</Project>